{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "@api/*": ["api/*"],
      "@mmkv/*": ["mmkv/*"],
      "@redux/*": ["redux/*"],
      "@style/*": ["style/*"],
      "@screen/*": ["screen/*"],
